{
  "name": "github-components",
  "version": "1.0.0",
  "description": "An api to dinamically generate svg componentes for my github readme",
  "main": "src/index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "build": "rimraf api && tsc",
    "start": "node api/index.js",
    "format": "npx prettier --write ./src",
    "ts.check": "tsc --project tsconfig.json"
  },
  "keywords": [
    "github",
    "components",
    "spotify"
  ],
  "author": "unchainedDavid",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
